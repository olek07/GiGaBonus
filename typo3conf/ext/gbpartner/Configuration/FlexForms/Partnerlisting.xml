<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>examples.pi_flexform.sheetGeneral</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.partners>
						<TCEforms>
							<label>Selected Partner</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>tx_gbpartner_domain_model_partner</allowed>
								<size>10</size>
								<maxitems>50</maxitems>
								<minitems>0</minitems>

                                <foreign_table>tx_gbpartner_domain_model_partner</foreign_table>

                                <foreign_table_where>AND (sys_language_uid=0 OR sys_language_uid = -1) AND tx_gbpartner_domain_model_partner.deleted = 0 AND tx_gbpartner_domain_model_partner.hidden = 0</foreign_table_where>

                                <show_thumbs>1</show_thumbs>
								<wizards>
									<suggest>
										<type>suggest</type>
									</suggest>
								</wizards>
							</config>
						</TCEforms>
					</settings.partners>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
