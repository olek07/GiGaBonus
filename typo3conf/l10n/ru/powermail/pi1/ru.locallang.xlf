      <target state="translated">По IP адресу %s вход уже сделан</target></trans-unit>
      <trans-unit id="error_expression_validation" xml:space="preserve" approved="yes">
        <source>Validation failed</source>
      <target state="translated">Введены некорректные данные</target></trans-unit>
