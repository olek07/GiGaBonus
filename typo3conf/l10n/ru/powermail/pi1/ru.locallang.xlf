      <target state="translated">Несуществующий адрес электронной почты</target></trans-unit>
      <trans-unit id="error_unique_field" xml:space="preserve" approved="yes">
        <source>%s was already used</source>
      <target state="translated">%s уже использовался</target></trans-unit>
