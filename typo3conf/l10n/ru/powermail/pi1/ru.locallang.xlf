      <target state="translated">Пожалуйста, выберите один из предложенных ниже вариантов</target></trans-unit>
      <trans-unit id="JSvalidation_label_validate_max" xml:space="preserve" approved="yes">
        <source>Please enter a value greater than $1</source>
      <target state="translated">Введите значение больше $1</target></trans-unit>
      <trans-unit id="JSvalidation_label_validate_min" xml:space="preserve" approved="yes">
        <source>Please enter a value smaller than $1</source>
      <target state="translated">Введите значение меньше $1</target></trans-unit>
